.headIcon {
  margin-top: 80rpx;
}
.navigation{
  position:fixed; 
  bottom:0;
  z-index: 9999;
  display: flex;
  align-items: center;
  background: #fffffc;
  height: 90rpx;
  width: 100%;
}
.navigation navigator{
  display: flex;
  flex-direction: column;
  align-items: center;
  line-height: 90rpx;
  width: 33%;
  height: 100%;
  font-size: 32rpx;
  color: #251f1f;
}
.cutLine{
  width: 4rpx;
  height: 28rpx;
  background-color: #8B8B8B;
}
.ap-address{
  color: #3B3D3C;
  font-size: 20rpx;
}
.last-ap{
  display: inline-block;
  width: 30rpx;
  height: 100%;
  background: black;
}

.nickArea {
  margin-top: 80rpx;
  width: 100%;
  display: flex;
  color: white;
  bottom: 60rpx;
  align-items: center;
}

.authBtn {
  background: transparent;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 28rpx;
  width: 220rpx;
  color: black;
}